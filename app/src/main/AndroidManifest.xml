<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meauapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdocaoSucesso"></activity>
        <activity android:name=".PaginaAnimal" />
        <activity android:name=".CadastroAnimal" />
        <activity android:name=".Cadastro" />
        <activity android:name=".Login2" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Introducao" />
        <activity
            android:name=".Adotar"
            android:parentActivityName=".Introducao">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.meauapp.Introducao" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>